#Media aritmética de 3 números
.text
.globl main
main:
addi $t1,$0,20
addi $t2,$0,30
addi $t3,$0,40
add $t4,$0,$t1
add $t4,$t4,$t2
add $t4,$t4,$t3
addi $t5,$t5,3
div $t4,$t5
mflo $t6

li $v0,1 # El valor 1 indica que se imprimirá un valor entero
addi $a0,$t6,0 # Alamcenamos en $a0 el valor a imprimir (Media+0)
syscall

